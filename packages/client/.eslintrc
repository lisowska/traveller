{
  "root": true,
  "extends": ["../../.eslintrc"],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "settings": {
    "babel": true,
    "compat": true
  },
  "ignorePatterns": ["/dist"],
  "rules": {
    "semi": ["error", "never"],
    "@typescript-eslint/semi": "off",
    "no-unexpected-multiline": "error"
  }
}
